{
    "manifest_version": 3,
    "name": "Tab Control",
    "version": "1.0",
    "description": "Close tabs to the left and limit the number of open tabs.",
    "permissions": [
        "tabs",
        "contextMenus",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "options.html"
    },
    "icons": {
        "48": "tab-control-48.png",
        "128": "tab-control-128.png"
    }
}